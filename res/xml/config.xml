<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ctj.swk.Myloan" version="1.0.9" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Clipboard">
        <param name="android-package" value="com.verso.cordova.clipboard.Clipboard" />
    </feature>
    <feature name="Contacts">
        <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
    </feature>
    <feature name="Permissions">
        <param name="android-package" value="com.android.plugins.Permissions" />
    </feature>
    <feature name="NativeStorage">
        <param name="android-package" value="NativeStorage" />
    </feature>
    <feature name="GeTuiSdkPlugin">
        <param name="android-package" value="com.igexin.getui.phonegap.GeTuiSdkPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="CheckAppUpdate">
        <param name="android-package" value="com.vaenow.appupdate.CheckAppUpdate" />
    </feature>
    <feature name="PackageCordovaPlugin">
        <param name="android-package" value="成都.pkg.PackageCordovaPlugin" />
    </feature>
    <feature name="CommUtil">
        <param name="android-package" value="com.plugin.br.cordova.CommUtil" />
    </feature>
    <feature name="ContactCordovaPlugin">
        <param name="android-package" value="com.plugin.br.cordova.contact.ContactCordovaPlugin" />
    </feature>
    <uses-permission android:name="android.permission.READ_CALL_LOG" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <feature name="EntryCordovaPlugin">
        <param name="android-package" value="com.plugin.br.cordova.entry.EntryCordovaPlugin" />
    </feature>
    <feature name="FacePlugin">
        <param name="android-package" value="com.plugin.br.cordova.face.FacePlugin" />
    </feature>
    <feature name="AppsFlyerPlugin">
        <param name="android-package" value="com.plugin.br.cordova.appsfyler.AppsFlyerPlugin" />
    </feature>
    <feature name="DeviceBr">
        <param name="android-package" value="com.plugin.br.cordova.device.DeviceBr" />
        <uses-permission android:name="android.permission.INTERNET" />
        <uses-permission android:name="android.permission.READ_PHONE_STATE" />
        <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
        <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
        <uses-permission android:name="android.Manifest.permission.READ_PHONE_STATE" />
        <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
        <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="GoogleMapPlugin">
        <param name="android-package" value="com.plugin.br.cordova.googlemapplugin.GoogleMapPlugin" />
        <uses-permission android:name="android.permission.INTERNET" />
        <uses-permission android:name="android.permission.READ_PHONE_STATE" />
        <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
        <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
        <uses-permission android:name="android.Manifest.permission.READ_PHONE_STATE" />
        <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
        <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    </feature>
    <allow-navigation href="http://localhost:8080/*" />
    <feature name="IonicWebView">
        <param name="android-package" value="com.ionicframework.cordova.webview.IonicWebView" />
    </feature>
    <feature name="Keyboard">
        <param name="android-package" onload="true" value="io.ionic.keyboard.IonicKeyboard" />
    </feature>
    <feature name="RareloopAppVersion">
        <param name="android-package" value="com.rareloop.cordova.appversion.RareloopAppVersion" />
    </feature>
    <name>Myloan</name>
    <description>An awesome Ionic/Cordova app.</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://192.168.33.4:8100" />
    <allow-navigation href="http://10.0.52.67:8100" />
    <chcp>
        <config-file url="https://s3-sa-east-1.amazonaws.com/hujin2-s1/apk/br/resources-MyLoan/www/chcp.json" />
        <auto-download enabled="false" />
        <native-interface version="1" />
    </chcp>
    <allow-intent href="market:*" />
    <icon density="ldpi" src="resources/android/icon/logo.png" />
    <icon density="mdpi" src="resources/android/icon/logo.png" />
    <icon density="hdpi" src="resources/android/icon/logo.png" />
    <icon density="xhdpi" src="resources/android/icon/logo.png" />
    <icon density="xxhdpi" src="resources/android/icon/logo.png" />
    <icon density="xxxhdpi" src="resources/android/icon/logo.png" />
    <splash density="land-ldpi" src="resources/android/splash/splash.png" />
    <splash density="land-mdpi" src="resources/android/splash/splash.png" />
    <splash density="land-hdpi" src="resources/android/splash/splash.png" />
    <splash density="land-xhdpi" src="resources/android/splash/splash.png" />
    <splash density="land-xxhdpi" src="resources/android/splash/splash.png" />
    <splash density="land-xxxhdpi" src="resources/android/splash/splash.png" />
    <splash density="port-ldpi" src="resources/android/splash/splash.png" />
    <splash density="port-mdpi" src="resources/android/splash/splash.png" />
    <splash density="port-hdpi" src="resources/android/splash/splash.png" />
    <splash density="port-xhdpi" src="resources/android/splash/splash.png" />
    <splash density="port-xxhdpi" src="resources/android/splash/splash.png" />
    <splash density="port-xxxhdpi" src="resources/android/splash/splash.png" />
    <config-file parent="/manifest" target="AndroidManifest.xml">
        <uses-permission android:name="android.permission.SEND_SMS" />
        <uses-permission android:name="android.permission.READ_CALL_LOG" />
        <uses-permission android:name="android.permission.READ_SMS" />
        <uses-permission android:name="android.permission.CAMERA" />
        <uses-permission android:name="android.permission.INTERNET" />
    </config-file>
    <preference name="loglevel" value="DEBUG" />
    <preference name="webView" value="com.ionicframework.cordova.webview.IonicWebViewEngine" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="ShowSplashScreen" value="true" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
</widget>
